{
  "name": "ai-reworder-extension",
  "displayName": "Ai reworder extension",
  "version": "0.0.0",
  "description": "A OpenAI GPT-3 AI extension.",
  "author": "Mark Kretschmann <kretschmann@kde.org>",
  "packageManager": "pnpm@7.13.1",
  "scripts": {
    "dev": "plasmo dev",
    "build": "plasmo build"
  },
  "dependencies": {
    "@emotion/cache": "11.10.3",
    "@emotion/react": "11.10.4",
    "@emotion/styled": "11.10.4",
    "@mui/icons-material": "5.10.9",
    "@mui/material": "5.10.8",
    "@plasmohq/storage": "0.12.1",
    "plasmo": "0.55.2",
    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.19.3",
    "@plasmohq/prettier-plugin-sort-imports": "1.2.2",
    "@types/chrome": "0.0.196",
    "@types/node": "18.7.15",
    "@types/react": "18.0.18",
    "@types/react-dom": "18.0.6",
    "prettier": "2.7.1",
    "typescript": "4.8.2"
  },
  "manifest": {
    "permissions": [
      "activeTab",
      "clipboardRead",
      "clipboardWrite",
      "scripting",
      "storage",
      "tabs"
    ],
    "sandbox": {
      "pages": [
        "~sandbox.html"
      ]
    },
    "host_permissions": [
      "https://*/*"
    ]
  }
}
